    "jest:watch": "jest --watch",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "test": "tsc --noEmit && npm run prettier:write && yarn run markdownlint-cli2  --config .markdownlint-cli2.mjs && yarn run lint"
  },
  "dependencies": {
    "@mantine/core": "8.3.10",
